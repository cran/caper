	Doi = {DOI 10.1098/rspb.2006.3700},
	Journal = {Proceedings of the Royal Society B-Biological Sciences},
	Keywords = {diversity gradients; South American avifauna; climate-based models; species richness patterns; mid-domain effect; spatially explicit stochastic models},
	Pages = {165-174},
	Publisher = {ROYAL SOCIETY},
	Timescited = {20},
