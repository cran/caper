	Journal = {Proceedings of the National Academy of Sciences},
	Number = {46},
	Pages = {17836 --17841},
	Title = {{Linking global turnover of species and environments}},
	Url = {http://www.pnas.org/content/early/2008/11/10/0803524105.abstract},
