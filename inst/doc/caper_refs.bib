	Journal = {Ecology Letters},
	Keywords = {biogeography; climate-richness relationship; hotspots; latitudinal gradient of diversity; mid-domain effect; more-individuals hypothesis; mountains; productivity; spatial models; species-energy relationship},
	Pages = {1308-1320},
	Timescited = {0},
	Title = {Energy, range dynamics and global species richness patterns: reconciling mid-domain effects and environmental determinants of avian diversity},
	Volume = {9},
