	Doi = {DOI 10.1098/rspb.2007.1106},
	Journal = {Proceedings of the Royal Society B-Biological Sciences},
	Keywords = {elevation; climate; latitude; niche evolution; speciation; species richness},
	Pages = {2995-3003},
	Publisher = {ROYAL SOC},
	Timescited = {2},
	Title = {Climatic zonation drives latitudinal variation in speciation mechanisms},
	Volume = {274},
	Year = {2007},
