@article{Phillimore.Freckleton.ea.2006.a,
	Abstract = {One of the most striking patterns in evolutionary biology is that clades may differ greatly in the number of species they contain. Numerous hypotheses have been put forward to explain this phenomenon, and several have been tested using phylogenetic methods. Remarkably, however, all such tests performed to date have been characterized by modest explanatory power, which has generated an interest in explanations stressing the importance of random processes. Here we make use of phylogenetic methods to test whether ecological variables, typically ignored in previous models, may explain phylogenetic tree imbalance in birds. We show that diversification rate possesses an intermediate phylogenetic signal across families. Using phylogenetic comparative methods, we then build a multipredictor model that explains more than 50\% of the variation in diversification rate among clades. High annual dispersal is identified as the strongest predictor of high rates of diversification. In addition, high diversification rate is strongly associated with feeding generalization. In all but one instance, these key findings remain qualitatively unchanged when we use an alternative phylogeny and methodology and when small clades, containing five species or less, are excluded. Taken together, these results suggest that large- scale patterns in avian diversification can be explained by variation in intrinsic biology.},
	Author = {Phillimore, Albert B. and Freckleton, Robert P. and Orme, C. David L. and Owens, Ian P. F.},
	Date-Added = {2008-02-07 15:04:30 +0000},
	Date-Modified = {2008-02-07 15:05:03 +0000},
	Journal = {American Naturalist},
	Keywords = {birds; diversification; phylogeny; ecology; dispersal; body size},
	Pages = {220-229},
	Timescited = {0},
	Title = {Ecology predicts large-scale patterns of phylogenetic diversification in birds},
	Volume = {168},
	Year = {2006},
	Bdsk-File-1 = {}}

@article{Davies.Orme.ea.2006.a,
	Abstract = {Understanding the global geographical distribution of extinction risk is a key challenge in conservation biology. It remains controversial, however, to what extent areas become threat hotspots simply because of high human impacts or due to predisposing ecological conditions. Limits to the taxonomic and geographical extent, resolution and quality of previously available data have precluded a full global assessment of the relative roles of these factors. Here, we use a new global database on the geographical distributions of birds on continents and continental islands to show that, after controlling for species richness, the best predictors of the global pattern of extinction risk are measures of human impact. Ecological gradients are of secondary importance at a global scale. The converse is true for individual biogeographic realms, within which variation in human impact is reduced and its influence on extinction risk globally is therefore underestimated. These results underline the importance of a global perspective on driving spatial patterns of extinction risk, and the key role of anthropogenic factors in driving the current extinction crisis.},
	Author = {Davies, Richard G. and Orme, C. David L. and Olson, Valerie and Thomas, Gavin H. and Ross, Simon G. and Ding, Tzung-Su and Rasmussen, Pamela C. and Stattersfield, Ali J. and Bennett, Peter M. and Blackburn, Tim M. and Owens, Ian P. F. and Gaston, Kevin J.},
