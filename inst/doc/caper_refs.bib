	Keywords = {habitat loss; landscape scale; habitat configuration; patch size; patch isolation; extinction threshold; landscape complementation},
	Pages = {487-515},
	Publisher = {ANNUAL REVIEWS},
	Timescited = {360},
	Title = {Effects of habitat fragmentation on biodiversity},
