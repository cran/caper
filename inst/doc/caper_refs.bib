	Month = {Jul},
	Number = {7304},
	Pages = {365-7},
	Pmid = {20592729},
	Pst = {ppublish},
	Title = {Replacing underperforming protected areas achieves better conservation outcomes},
	Volume = {466},
	Year = {2010},
